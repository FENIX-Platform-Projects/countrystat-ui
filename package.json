{
  "name": "countrystat-ui",
  "version": "1.0.0",
  "description": "Repo for countrystat controller",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "echo \"Error: please specify a valid section to develop [e.g. npm run dev:analysis]\" && exit 1",
    "dev:analysis": "SECTIONS=analysis NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:data_management": "SECTIONS=data_management NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "w-dev:data_managment" : "SET NODE_ENV=develop&&SET SECTION=data_management&&webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "build": "SECTIONS=analysis,data_management NODE_ENV=production webpack --display-error-details --profile --json  > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FENIX-Platform-Projects/countrystat-ui.git"
  },
  "author": "Barbara Cintoli <barbara.cintoli@fao.org> (http://fenix.fao.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FENIX-Platform-Projects/countrystat-ui/issues"
  },
